{
  "name": "tavuelo",
  "version": "0.12.14",
  "description": "Easy to use but with advanced features a Vue.js table component.",
  "private": false,
  "author": "lukaszkups",
  "homepage": "https://lukaszkups.net/notes/?category=tavuelo",
  "keywords": [
    "vue",
    "vue.js",
    "table",
    "datatable",
    "data",
    "data table",
    "plugin",
    "table plugin",
    "grid"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukaszkups/tavuelo.git"
  },
  "main": "dist/tavuelo.umd.min.js",
  "license": "MIT",
  "files": [
    "dist/*",
    "src/components/*",
    "public/*"
