    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "acorn": "^8.0.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-plugin-vue": "^5.0.0",
    "minimist": "^1.2.6",
    "sass": "^1.52.2",
    "sass-loader": "^9.0.3",
    "vue-template-compiler": "^2.6.12"
